{
  "name": "koishi-plugin-pics",
  "description": "Koishi 的随机图片插件",
  "version": "9.3.8",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "build": "webpack && env PACK_ALL=1 webpack",
    "start": "ts-node ./dev",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koishijs/koishi-plugin-pics.git"
  },
  "author": "Nanahira <nanahira@momobako.com>",
  "license": "MIT",
  "keywords": [
    "Koishi.js",
    "qqbot",
    "cqhttp",
    "onebot"
  ],
  "bugs": {
    "url": "https://github.com/koishijs/koishi-plugin-pics/issues"
  },
  "homepage": "https://github.com/koishijs/koishi-plugin-pics",
  "dependencies": {
    "koishi-thirdeye": "^10.2.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@koishijs/plugin-cache-lru": "^1.0.0-rc.0",
    "@koishijs/plugin-console": "^3.3.2",
    "@koishijs/plugin-database-memory": "^1.3.0",
    "@koishijs/plugin-sandbox": "^1.1.3",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "ws": "^8.3.0"
  },
  "peerDependencies": {
    "koishi": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "koishi": {
    "service": {
      "optional": [
        "assets"
      ],
      "implements": [
        "pics"
      ]
    },
    "locales": [
      "en",
      "zh"
    ],
    "recommends": [
      "picsource-lolicon"
    ]
  }
}
